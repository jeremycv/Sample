
<table width=100% border=0 cellpadding=0 cellspacing=0>
	<tr class="someclass">
		<td class="Underline" width=40%><%= link_to(team.name, team) %></td>
		<td class="Underline" width=35%><%= team.business_unit %></td>
		<% status = team.status %>
		<% colour = team.colour(status) %>
		<td class="Underline" width=25%><font color=<%= "#{colour}" %>><%= status %></font></td>
	</tr>
</table>
